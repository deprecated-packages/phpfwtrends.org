{
    "name": "tomasvotruba/phpfwtrends-org",
    "license": "MIT",
    "description": "Site with statistics of PHP framework downloads - raw Packagist data over guessing and emotions",
    "require": {
        "php": ">=8.0",
        "doctrine/cache": "^1.10",
        "guzzlehttp/guzzle": "^7.2",
        "j7mbo/twitter-api-php": "^1.0",
        "nette/finder": "^2.5",
        "nette/utils": "^3.2",
        "spatie/packagist-api": "^2.0.2",
        "symfony/asset": "^5.2",
        "symfony/cache": "^5.2",
        "symfony/console": "^5.2",
        "symfony/debug-bundle": "^5.2",
        "symfony/dotenv": "^5.2",
        "symfony/framework-bundle": "^5.2",
        "symfony/http-foundation": "^5.2",
        "symfony/http-kernel": "^5.2",
        "symfony/twig-bridge": "^5.2",
        "symfony/twig-bundle": "^5.2",
        "symplify/amnesia": "^9.4",
        "symplify/easy-hydrator": "^9.4",
        "symplify/package-builder": "^9.4",
        "symplify/php-config-printer": "^9.4",
        "symplify/smart-file-system": "^9.4",
        "symplify/symfony-static-dumper": "^9.4",
        "twig/twig": "^3.3"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.93",
        "phpstan/phpstan-webmozart-assert": "^0.12.12",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.11.39",
        "symplify/coding-standard": "^9.4",
        "symplify/easy-coding-standard": "^9.4",
        "symplify/phpstan-extensions": "^9.4",
        "symplify/phpstan-rules": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "TomasVotruba\\PhpFwTrends\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TomasVotruba\\PhpFwTrends\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse --ansi --error-format symplify",
        "check-cs": "vendor/bin/ecs check --ansi",
        "fix-cs": "vendor/bin/ecs check --fix --ansi",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "sort-packages": true
    }
}
